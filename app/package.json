{
  "name": "dolar-app",
  "version": "1.0.0",
  "description": "An Amazing Capacitor App",
  "main": "index.js",
  "type": "module",
  "keywords": [
    "capacitor",
    "mobile"
  ],
  "scripts": {
    "copy-app-files": "zx scripts/copy-app-files.js",
    "build": "vite build",
    "dev": "VITE_DEV=1 vite --host --port 3000",
    "dev:build": "VITE_DEV=1 vite build",
    "dev:android": "VITE_DEV=1 cap run android",
    "android": "cap run android",
    "preview": "vite preview --host --port 3000",
    "assets": "npx @capacitor/assets generate --iconBackgroundColor='#4bae4f' --iconBackgroundColorDark='#4bae4f' --splashBackgroundColor='#4bae4f' --splashBackgroundColorDark='#4bae4f'",
    "prepare": "ts-patch install && typia patch",
    "check": "svelte-check --tsconfig ./tsconfig.json --threshold error"
  },
  "dependencies": {
    "@capacitor-community/in-app-review": "^6.0.0",
    "@capacitor/android": "^6.0.0",
    "@capacitor/app": "^6.0.0",
    "@capacitor/assets": "^3.0.5",
    "@capacitor/core": "^6.0.0",
    "@capacitor/dialog": "^6.0.0",
    "@capacitor/filesystem": "^6.0.0",
    "@capacitor/haptics": "^6.0.0",
    "@capacitor/share": "^6.0.0",
    "@capacitor/splash-screen": "^6.0.0",
    "@capawesome/capacitor-app-update": "^6.0.0",
    "@capawesome/capacitor-screen-orientation": "^6.0.0",
    "@mdi/js": "^7.4.47",
    "@sveltejs/pancake": "^0.0.18",
    "copy-to-clipboard": "^3.3.3",
    "html2canvas": "^1.4.1",
    "pancake": "./packages/pancake/package",
    "svelte-check": "^3.7.1",
    "typia": "^6.7.2"
  },
  "devDependencies": {
    "@capacitor/cli": "^6.0.0",
    "@sveltejs/adapter-static": "^3.0.1",
    "@sveltejs/vite-plugin-svelte": "^4.0.0",
    "cssnano": "^7.0.1",
    "postcss": "^8.4.38",
    "rollup": "^4.17.2",
    "rollup-plugin-typescript": "^1.0.1",
    "svelte": "^5.0.0",
    "ts-patch": "^3.2.1",
    "typescript": "^5.5.2",
    "vite": "^5.2.10",
    "zx": "^8.0.2"
  },
  "author": "",
  "license": "ISC"
}